{
  "$schema": "../coil-monorepo-upkeep/resources/package-json-schema-nested-overrides.json",
  "name": "@web-monetization/demo",
  "version": "0.0.0",
  "keywords": [
    "ilp",
    "web-monetization"
  ],
  "homepage": "https://github.com/coilhq/web-monetization-projects/tree/main/packages/web-monetization-demo",
  "repository": {
    "type": "git",
    "url": "git@github.com:coilhq/web-monetization-projects.git"
  },
  "license": "Apache-2.0",
  "author": "Coil Team <info@coil.com>",
  "main": "./build",
  "types": "./build",
  "scripts": {
    "build:ts": "tsc --build tsconfig.build.json",
    "build:ts:verbose": "yarn build:ts --verbose",
    "build:ts:watch": "yarn build:ts --watch",
    "clean:build": "rimraf build",
    "client:dev": "webpack-cli serve --mode development --hot",
    "format": "yarn prettier && LINT_FIX=1 yarn lint:all --fix --quiet",
    "lint": "eslint --cache --cache-location ../../node_modules/.cache/eslint",
    "lint:all": "yarn lint 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "precommit": "echo lint-staged runs from root",
    "prettier": "prettier --write '*.{ts,tsx,js,html,jsx,md}' '{src,test}/**/*.{ts,tsx,js,html,jsx,md}'",
    "receipt-verifier": "REDIS_URI=mock SPSP_ENDPOINT=http://localhost:4000/spsp/~niq SPSP_PROXY_PORT=4001 VERIFIER_PORT=4002 receipt-verifier",
    "receiver:dev": "ts-node-dev -r tsconfig-paths/register -P test/tsconfig.json --respawn --transpile-only src/receiver/main.ts",
    "test": "jest --passWithNoTests",
    "test:coverage": "yarn test --verbose --coverage",
    "upkeep": "cd ../.. && yarn upkeep",
    "verifier:dev": "ts-node-dev -r tsconfig-paths/register -P test/tsconfig.json --respawn --transpile-only src/verifier/main.ts"
  },
  "dependencies": {
    "@abraham/reflection": "^0.7.0",
    "@coil/receipt-verifier": "^1.0.5",
    "@web-monetization/react": "0.0.12",
    "@web-monetization/types": "0.0.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "ilp-plugin-btp": "^1.4.2",
    "ilp-plugin-mini-accounts": "^4.2.0",
    "ilp-protocol-stream": "^2.6.4",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "node-fetch": "^2.6.1",
    "oer-utils": "^5.1.2",
    "react": "^17.0.1",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@coil/webpack-utils": "^0.0.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@hot-loader/react-dom": "^16.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.10.3",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.0",
    "JSON2016": "^1.0.0",
    "copy-webpack-plugin": "^6.3.2",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^4.1.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.1",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2",
    "webpack": "^5.6.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0"
  }
}
